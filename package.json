{
  "name": "svg-navigator---chrome-extension",
  "version": "1.0.0",
  "description": "SVG Navigator is an extension for Google Chrome that adds pan and zoom features to existing SVG files on the web.",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "build:chrome": "zip -r extension.zip src",
    "build:firefox": "web-ext build",
    "web-ext:lint": "web-ext lint",
    "start:firefox": "web-ext run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pRizz/SVG-Navigator---Chrome-Extension.git"
  },
  "keywords": [
    "svg"
  ],
  "author": "Peter Ryszkiewicz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pRizz/SVG-Navigator---Chrome-Extension/issues"
  },
  "homepage": "https://github.com/pRizz/SVG-Navigator---Chrome-Extension#readme",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@webextension-toolbox/webextension-toolbox": "^7.1.1",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "web-ext": "^8.4.0"
  }
}
