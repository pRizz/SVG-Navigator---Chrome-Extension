{
    "name": "SVG Viewer",
    "description": "This extension extends the built in SVG viewer in Chrome by adding zooming and panning capabilities.",
    "version": "1.0",
    "manifest_version": 2,
        
    "page_action": {
        "default_title": "SVG Viewer"
    },
    
    "background": {
        "scripts": ["svgViewer.js"],
        "persistent": false
    },
    
    "content_scripts": [
    {
        "matches": ["http://*/*.svg"],
        "js": ["svgViewer.js"],
        "run_at": "document_end"
    }
    ]
    

}