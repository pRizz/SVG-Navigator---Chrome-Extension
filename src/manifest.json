{
    "name": "SVG Navigator",
    "short_name": "SVG Nav",
    "description": "Adds zooming and panning capabilities to SVG files",
    "version": "2.4.3",
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "background": {
        "service_worker": "service_worker.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*.svg",
                "https://*/*.svg",
                "ftp://*/*.svg",
                "file://*/*.svg",
                "http://*/*.svgz",
                "https://*/*.svgz",
                "ftp://*/*.svgz",
                "file://*/*.svgz"
            ],
            "js": ["options_custom/js/defaultSettings.js", "js/svgNavigator.js"],
            "css": ["css/svgNavigator.css"]
        }
    ],
    "manifest_version": 3,
    "action": {
        "default_popup": "options_custom/index.html",
        "default_title": "SVG Navigator",
        "default_icon": {
            "16": "icon_16.png",
            "48": "icon_48.png"
        }
    },
    "permissions": [
        "storage"
    ]
}
